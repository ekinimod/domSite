{% set messagePreTitle="TmplDom" %}
<!DOCTYPE html>
<html>
    <!-- Head -->
    <head>
        <!-- Meta -->
        {% block metas %}
            {% include "default/meta.html.twig" %}
        {% endblock %}
        <!-- Title -->
        {% include "default/title-page.html.twig" %}
        <!-- Css -->
        <link href="{{ asset('build/app.css') }}" type="text/css" rel="stylesheet"/>
    </head>
    <!-- Body -->
    <body>
        <!-- Ligne Entete -->
        {% include "default/logo.html.twig" %}
        {% include "default/titre-page.html.twig" %}
