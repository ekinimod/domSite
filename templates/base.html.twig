        <!-- Ligne Breadcrumb -->
        {% include "default/horloge.html.twig" %}
        {% include "default/breadcrumb.html.twig" %}
        {% include "default/menu-notif.html.twig" %}
        <!-- Ligne menu & contenu  -->
        {% include "default/menu-gauche.html.twig" %}
        {% block content %}{% endblock %}
        <!-- Ligne PiedDePage -->
        {% include "default/footer.html.twig" %}
        <!-- Script & fin -->
        <script src="{{ asset('build/app.js') }}"></script>
    </body>
</html>
